{
  "$schema": "https://json.schemastore.org/tsconfig",
"extends": "@tsconfig/node20/tsconfig.json",
  "compileOnSave": false,
  "compilerOptions": {
"strict": true,
"resolveJsonModule": true,
"isolatedModules": true,
"esModuleInterop": true,
"noEmit": true,
"noUnusedLocals": true,
"noUnusedParameters": true,
"noImplicitReturns": true,
"jsx": "preserve",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
"allowUmdGlobalAccess": true,
"verbatimModuleSyntax": true,
"stripInternal": true,
"paths": {
  "@docs/*": [
    "apps/docs/src/*"
  ],
  "@sln-api/*": [
    "apps/sln-api/src/*"
  ],
  "@sln-next-web/*": [
    "apps/sln-next-web/src/*"
  ],
  "@sln-next-web/server/*": [
    "apps/sln-next-web/server/*"
  ],
},
"types": [
  "vite/client"
]
  },
"ignore": [
  "node_modules",
  ".cache",
  "dist",
  ".yarn"
],
  "exclude": [
    "node_modules",
".cache",
"dist",
".yarn",
    "tmp"
  ]
}
